{
    "name": "jsamson/php-cs-custom-fixer",
    "description": "Custom fixers for php-cs-fixer",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "friendsofphp/php-cs-fixer": "^2.16.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jérémie Samson",
            "email": "jeremie.samson76@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "JSamson\\CS\\Fixer\\": "src/JSamson/CS/Fixer"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2@dev",
        "symfony/var-dumper": "^5.1@dev"
    }
}
